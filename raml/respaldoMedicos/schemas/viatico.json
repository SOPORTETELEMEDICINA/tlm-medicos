{
  "$schema": "http://json-schema.org/draft-03/schema#",
  "title": "viatico",
  "type": "object",  
  "properties": {
    "idReg": {
      "type": "integer",
      "required": false,
      "title": "idReg"
    },
    "compania": {
      "type": "integer",
      "required": false,
      "title": "compania"
    },
    "emplContable": {
      "type": "string",
      "required": false,
      "title": "emplContable"
    },
    "dip": {
      "type": "string",
      "required": false,
      "title": "dip"
    },
    "monto": {
      "type": "number",
      "required": true,
      "title": "monto",
      "minimum": 1
    },
    "fechaRegistro": {
      "type": "string",
      "required": false,
      "title": "fechaRegistro"
    },
    "concepto": {
      "type": "string",
      "required": false,
      "title": "concepto"
    },
    "origen": {
      "type": "object",
      "properties": {
        "appId": {
          "type": "string",
          "required": true,
          "title": "appId"
        },
        "usuario": {
          "type": "string",
          "required": true,
          "title": "usuario"
        },
        "nota": {
          "type": "string",
          "required": true,
          "title": "nota"
        }
      }
    }
  }
}